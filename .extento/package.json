{
    "name": "extento-internal",
    "version": "0.0.1",
    "engineStrict": true,
    "engines": {
        "npm": "8.15.0",
        "node": "18.7.0"
    },
    "scripts": {
        "start": "trap 'exit 0' SIGINT; COMMAND=compileDev node ./node/commands/bin.js",
        "init": "node ./node/scripts/resetSelectiveLayers.js",
        "poststart": "npm run init",
        "postinstall": "npx link-module-alias",
        "build:release": "EXTENTO_DEV=false node ./node/scripts/buildLayers",
        "build-all:release": "EXTENTO_DEV=false EXTENTO_BUILD_ALL=true node ./node/scripts/buildLayers",
        "build:dev": "EXTENTO_DEV=true node ./node/scripts/buildLayers",
        "build-all:dev": "EXTENTO_DEV=true EXTENTO_BUILD_ALL=true node ./node/scripts/buildLayers",
        "lint": "npx eslint ../layers ../shared ../pages"
    },
    "_moduleAliases": {
        "~": "."
    },
    "devDependencies": {
        "@types/chrome": "^0.0.204",
        "@types/lodash": "^4.14.190",
        "@types/node": "^18.6.5",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "ajv": "^8.11.2",
        "autoprefixer": "^10.4.8",
        "css-loader": "^6.7.2",
        "eslint": "^8.29.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "fs-extra": "^10.1.0",
        "html-loader": "^4.2.0",
        "html-webpack-plugin": "^5.5.0",
        "link-module-alias": "^1.2.0",
        "lodash": "^4.17.21",
        "markdown-loader": "^8.0.0",
        "postcss": "^8.4.19",
        "postcss-loader": "^7.0.1",
        "prettier": "^2.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "serialize-error": "^11.0.0",
        "signale": "^1.4.0",
        "style-loader": "^3.3.1",
        "tailwindcss": "^3.2.4",
        "terser-webpack-plugin": "^5.3.6",
        "to-string-loader": "^1.2.0",
        "ts-loader": "^9.4.1",
        "tsconfig-paths-webpack-plugin": "^4.0.0",
        "typescript": "^4.9.4",
        "uuid": "^9.0.0",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.11.1",
        "webpack-merge": "^5.8.0",
        "webpack-watch-files-plugin": "^1.2.1",
        "zip-local": "^0.3.5",
        "husky": "^8.0.2"
    }
}

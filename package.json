{
    "name": "extento-app",
    "version": "0.0.1",
    "engineStrict": true,
    "engines": {
        "npm": "8.15.0",
        "node": "18.7.0"
    },
    "workspaces": [
        ".extento",
        "dev.scripts"
    ],
    "scripts": {
